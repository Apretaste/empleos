<div class="row">
	<div class="col s12">
		<h1><%= title %></h1>
		<span class="chip clear spacing"><i class="fas fa-tag"></i> <%= categories[category] %></span>
		<span class="chip clear"><i class="fas fa-calendar"></i> <%= moment(inserted).format('D [de] MMMM [del] YYYY') %></span>
		<p><%= description %></p>
		<p>Email: <%= email %></p>
	</div>
</div>

<!-- BUTTONS -->
<div class="bottom-space"></div>

<div class="fixed-action-btn">
	<a class="btn-floating waves-effect waves-light" onclick="apretaste.send({command:'EMPLEOS', data: {}})">
		<i class="material-icons">arrow_back</i>
	</a>
</div>
